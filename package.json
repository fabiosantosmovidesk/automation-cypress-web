{
  "name": "automation-cypress-web",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "cy:open": "cypress open",
    "lint": "eslint cypress/**/*.js",
    "lint:fix": "eslint cypress/**/*.js --fix",
    "cy:open:prd": "cypress open --env configFile=prd",
    "cy:open:uat": "cypress open --env configFile=uat",
    "cy:open:dev": "cypress open --env configFile=dev",
    "cy:test:prd": "cypress run --env configFile=prd",
    "cy:test:uat": "cypress run --env configFile=uat",
    "cy:test:dev": "cypress run --env configFile=dev",
    "allure:run:dev": "npx cypress run --env configFile=dev,allure=true",
    "allure:run:uat": "npx cypress run --env configFile=uat,allure=true",
    "allure:run:prd": "npx cypress run --env configFile=prd,allure=true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabiosantosmovidesk/automation-cypress-web.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/fabiosantosmovidesk/automation-cypress-web/issues"
  },
  "homepage": "https://github.com/fabiosantosmovidesk/automation-cypress-web#readme",
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@testing-library/cypress": "^8.0.3",
    "allure-commandline": "^2.18.1",
    "cpf": "^2.0.1",
    "cypress": "^10.9.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^2.6.1",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^3.2.2",
    "cypress-multi-reporters": "^1.6.1",
    "cypress-terminal-report": "^4.1.2",
    "cypress-xpath": "^2.0.1",
    "eslint": "^8.23.0",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha-junit-reporter": "^2.1.0"
  },
  "devDependencies": {
    "@shelex/cypress-allure-plugin": "^2.33.0"
  }
}
